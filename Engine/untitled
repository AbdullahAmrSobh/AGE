age_add_target(
    NAME AgeEngine
    STATIC
    NAMESPACE ENGINE
    FILES_CMAKE
        Files.cmake

    INCLUDE_DIRECTORIES
        PUBLIC 
            ${CMAKE_CURRENT_SOURCE_DIR}/Public
        PRIVATE 
            ${CMAKE_CURRENT_SOURCE_DIR}/Private
        INTERFACE
            ${CMAKE_CURRENT_SOURCE_DIR}/Interface

    COMPILE_DEFINITIONS
        AGE_DEBUG
        AGE_CORE
    
    BUILD_DEPENDENCIES
        EMPTY_BUILD_DEP
    RUNTIME_DEPENDENCIES
        EMPTY_RUNTIME_BUILD_DEP
    PLATFORM_INCLUDE_FILES
        EMPTY_PLATFORM_INCLUDE_FILES
    TARGET_PROPERTIES
        EMPTY_TARGET_PROPERTIES
    AUTOGEN_RULES
        EMNPTY_AUTOGEN_RULES
)